  "characteristics": {
    "TOT_NID_STORAGE_2013": {
      "Procedure": "getStoragePerUnitArea",
      "Description": "The maximum dam storage (in acre-feet) defined as the total storage space in a reservoir below the maximum attainable water surface elevation, including any surcharge storage. ",
      "MapLayers": ["nid2013"],
      "QueryField": "NID_STORAG",
      "UnitID": "acre feet",
      "Reference": "https://www.sciencebase.gov/catalog/item/57bf394ae4b0f2f0ceb759f1"
    },
    "TOT_FRESHWATER_WD": {
      "Procedure": "getRasterStatistic",
      "Description": "Freshwater withdrawal megaliters (1000 cubic meters) per year per sqkm, from 1995-2000 county-level estimates",
      "Method": "Mean",
      "MapLayers": [ "freshwater_wd" ],
      "UnitID": "megaliters/km2"
    },
    "TOT_IMPV11": {
      "Procedure": "getRasterStatistic",
      "Description": "Impervious surface from NLCD 2011",
      "Method": "Mean",
      "MapLayers": [ "nlcd2011" ],
      "UnitID": "percent"
    },
    "TOT_MIRAD_2012": {
      "Procedure": "getRasterPercent",
      "Description": "Percent of watershed in irrigated agriculture, from USGS 2012 250-m MODIS data",
      "MapLayers": [ "mirad2012" ],
      "UnitID": "percent",
      "Reference": "https://www.sciencebase.gov/catalog/item/57f66d66e4b0bc0bec06de3a"
    },
    "TOT_NWALT12_41": {
      "Procedure": "getRasterPercent",
      "Description": "Mining/Extraction - Barren areas due to probable surface mining/mineral extraction. Includes mining, quarries, sand and gravel operations, and energy operations.",
      "MapLayers": [ "nwalt2012" ],
      "ClassCodes": "41",
      "UnitID": "percent"
    },
    "TOT_DITCHES92": {
      "Procedure": "getVectorDensity",
      "Operator": "LIKE",
      "Keyword": "Canal",
      "Method": "SUM",
      "methodField": "LENGTHKM",
      "selectorField": "FTYPE",
      "Description": "Percent of watershed subjected to ditches for the year 1992",
      "MapLayers": [ "flowlines" ],
      "UnitID": "percent",
      "Reference": "https://www.sciencebase.gov/catalog/item/57067257e4b03f95a075ab0c"
    },
    "TOT_NPDES_MAJ_DENS": {
      "Procedure": "getPointFeatureDensity",
      "Description": "NPDES Major site density, count/totalArea",
      "MapLayers": [ "npdes" ],
      "UnitID": "cnt/sqkm"
    }
  },
