  "catchment": {
    "upstream": "upstreamcatchment",
    "downstream": "downstreamcatchment",
    "global": "global"
  },
  "characteristics": {
    "TOT_NID_STORAGE_1980": {
      "Procedure": "getFeatureStatistic",
      "Description": "Reservoir storage",
      "Method": "SUM",
      "MapLayers": [ "nid1980" ],
      "QueryField": "NORM_STORA",
      "UnitID": "Ac-Ft"
    },
    "TOT_NPDES_MAJ_DENS": {
      "Procedure": "getPointFeatureDensity",
      "Description": "NPDES Major site density, count/totalArea",
      "MapLayers": [ "npdes" ],
      "UnitID": "cnt/sqkm"
    },
    "TOT_NWALT12_41": {
      "Procedure": "getRasterPercent",
      "Description": "Mining/Extraction - Barren areas due to probable surface mining/mineral extraction. Includes mining, quarries, sand and gravel operations, and energy operations.",
      "MapLayers": [ "nwalt2012" ],
      "ClassCodes": "41",
      "UnitID": "percent"
    },
    "TOT_MIRAD_2002": {
      "Procedure": "getRasterPercent",
      "Description": "Percent of watershed in irrigated agriculture, from USGS 2002 250-m MODIS data",
      "MapLayers": [ "mirad2002" ],
      "ClassCodes": "1",
      "UnitID": "percent"
    },
    "TOT_IMPV11": {
      "Procedure": "getRasterStatistic",
      "Description": "Impervious surface from NLCD 2011",
      "Method": "Mean",
      "MapLayers": [ "nlcd2011" ],
      "UnitID": "percent"
    },
    "TOT_FRESHWATER_WD": {
      "Procedure": "getRasterStatistic",
      "Description": "Freshwater withdrawal megaliters (1000 cubic meters) per year per sqkm, from 1995-2000 county-level estimates",
      "Method": "Mean",
      "MapLayers": [ "freshWithdraw" ],
      "UnitID": "megaliters/km2"
    }
  },
